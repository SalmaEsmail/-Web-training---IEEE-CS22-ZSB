    <script>
let needle = "JS";
let haystack = ["PHP", "JS", "Python"];

if (haystack.indexOf(needle) != -1){
    console.log("Found")
}
if (haystack.includes(needle)){
    console.log("Found")
}
if(haystack.lastIndexOf(needle) != -1){
    console.log("Found")
}
// Write 3 Solutions
    </script>
